cd $job_results
$run_prefix
export PYTHONPATH=$flee_location:$$PYTHONPATH

/usr/bin/env > env.log

$run_command python3 run_par.py input_csv source_data $simulation_period simsetting.csv > out.csv

run_UNHCR_uncertainty="$UNHCR_uncertainty"
# covert to lowercase
run_UNHCR_uncertainty=$$(echo "$$run_UNHCR_uncertainty" | tr "[:upper:]" "[:lower:]")

if [ "$$run_UNHCR_uncertainty" = "true" ]
then
	python3 run_UNHCR_uncertainty.py input_csv source_data $simulation_period simsetting.csv > out_uncertainty.csv
fi
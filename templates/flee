cd $job_results
$run_prefix

if [ -z "$flee_location" ]
then
	echo "Please set $$flee_location in your deploy/machines_user.yml file."
else
	export PYTHONPATH=$flee_location:$$PYTHONPATH
fi

/usr/bin/env > env.log

python3 run.py input_csv source_data $simulation_period simsetting.csv > out.csv

run_UNHCR_uncertainty="$UNHCR_uncertainty"
if [[ "$${run_UNHCR_uncertainty,,}" == "true" ]]
then
	python3 run_UNHCR_uncertainty.py input_csv source_data $simulation_period simsetting.csv > out_uncertainty.csv
fi